/**
 * Test non-indexed feature assignment expressions
 **/
active class Expressions_Assignment_Feature {

    private x: Integer;
    private y: Integer[] sequence;
    
    private class C {
        public x: Integer;
        public y: Integer[] sequence;
    }
    
    private datatype D {
        public x: Integer;
        public y: Integer[] sequence;
    }
    
    private assoc A {
        public c: C;
        public n: Integer;
    }

    private assoc B {
        public d: D;
        public m: Integer;
    }

    public test(out c: C, out d: D, out n: Integer, out m: Integer,
        out self: Expressions_Assignment_Feature) {  
      c = new C();
      c.x = 2;
      c.y = 1;
      c.y = null;
      c.y = Integer[]{1,2};
      
      c.n = 3;
      n = A::n(c);
      
      d = new D(0, null);
      d.x = 2;
      d.y = 1;
      d.y = null;
      d.y = Integer[]{1,2};
      
      d.m = 3;
      m = B::m(d);
      
      this.x = 2;
      this.y = 1;
      this.y = null;
      this.y = Integer[]{1,2};
      self = this;
    }
  
} do {
    this.test(c,d,n);
}
      