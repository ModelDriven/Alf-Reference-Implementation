activity MassedThingTest() {
	wing1 = new MassedThing(100.0);
	wing2 = new MassedThing::copyOf(wing1); // Created a second wing, because 'containedThings' is unique by default.
	fuselage = new MassedThing(250.0);
	airplane = new MassedThing(MassedThing[]{wing1, wing2, fuselage});
	WriteLine("Airplane total mass is " + RealFunctions::ToString(airplane.getTotalMass()));
}

