activity Quicksort(in list: Integer[0..*] sequence):       
  Integer[0..*] sequence 
{
   if (list->isEmpty()) {                                  
    return null;                                          
  } else {
    x = list[1]??; 
    list1 = list->excludeAt(1);                                         
    return Quicksort(list1->select a (a < x))->           
           including(x)->
           union(Quicksort(list1->select b (b >= x)));
  }
}
