class Sorter<T specializes Comparable> {

  private list: List<T>;
  @Create public Sorter() { }
  @Create public Sorter(in list: List<T>) {
    this();
    this.list = list;
  }

  public append(in elements: List<T>) { 
    this.list.add(elements);
  }
  public sort() { }
  public getList(): List<T> {
    return this.list;
  }
  public clear() {
    this.list.clear();
  }
}
