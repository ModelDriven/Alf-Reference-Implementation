activity AssertList(in label: String, in list: any[*] sequence, in expected: any[*] sequence) {
    stdout = StandardOutputChannel.allInstances()[1];
    AssertTrue(label + "->size()==" + IntegerFunctions::ToString(expected->size()), 
        list->size()==expected->size());
    for (i in 1..IntegerFunctions::Min(list->size(),expected->size())) {
        stdout.write(label + "[" + IntegerFunctions::ToString(i) + "]==");
        stdout.write(expected[i]);
        AssertTrue("", list[i]==expected[i]);
    }
}