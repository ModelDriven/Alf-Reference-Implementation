Member StructuredMember():
{
  Token t;
  String c = null;
  StereotypeAnnotationList sl;
  String v = null;
  Member m;
}
{
  [ t = <DOCUMENTATION_COMMENT> { c = t.image; } ]
  sl = StereotypeAnnotations()
  [ t = <PUBLIC> { v = t.image; }]    
  m = PropertyDefinition()
  { m.addDocumentation(c);
    m.setAnnotations(sl);
    m.setVisibility(v);
    return m;
  }
