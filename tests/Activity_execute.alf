activity execute()
{ 
  //@parallel 
  {  
    'activity' = (Activity)(this.types[1]); 
    {
      group = new ActivityNodeActivationGroup();
      group.activityExecution = this; 
    }
    {
      this.activationGroup = group;
      group.activate('activity'.node, 'activity'.edge);
    }
  }
}
