activity Quicksort(in list: Integer[0..*] sequence):       
  Integer[0..*] sequence 
{
  x = list[1];                                          
  if (list->isEmpty()) {                                  
    return null;                                          
  } else {
    list->removeAt(1);
    return Quicksort(list->select a (a < x))->           
           including(x)->
           union(Quicksort(list->select b (b >= x)));
  }
}
