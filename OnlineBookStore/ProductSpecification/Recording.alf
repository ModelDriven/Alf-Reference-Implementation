namespace ProductSpecification;

/**
A Recording is a particular work (sound recording, performance, movie) that is
available for sale by the store. Often the same recording title is available
in multiple formats (e.g., a single film, "Jaws," may be available in DVD, VHS
video, and VHS video/letterboxed).
*/
class Recording {

  /**
  The actual title of the recording.
  */
  public title: String;
  
  @Create
  public Recording(
    in title: String
  ) {
    this.title = title;
  }
  
}
